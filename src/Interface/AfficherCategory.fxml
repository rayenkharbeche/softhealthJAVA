<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="Interface.AfficherCategoryController">
   <children>
      <Label layoutX="65.0" layoutY="31.0" prefHeight="41.0" prefWidth="106.0" text="Recherche : " />
      <TextField fx:id="recherche" layoutX="161.0" layoutY="39.0" />
      <Button fx:id="supp" layoutX="477.0" layoutY="39.0" mnemonicParsing="false" onAction="#ButtonSupprimer" text="Supprimer" />
      <TableView fx:id="tabcategory" layoutX="40.0" layoutY="92.0" prefHeight="200.0" prefWidth="503.0">
        <columns>
          <TableColumn fx:id="idtab" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="nomtab" onEditCommit="#Change_Nom" prefWidth="75.0" text="Nom" />
            <TableColumn fx:id="desctab" onEditCommit="#Change_Desc" prefWidth="260.0" text="Description" />
            <TableColumn fx:id="colortab" minWidth="0.0" prefWidth="92.0" text="Colors" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
